{
    "name": "dailex/dailex",
    "type": "project",
    "description": "Daikon CQRS & ES integration with the Silex framework.",
    "keywords": [ "daikon", "silex", "symfony", "cqrs", "event-sourcing", "mvc", "framework" ],
    "homepage": "https://github.com/daikon-cqrs/dailex",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Hasham Ahmad",
            "email": "mr.hash@gmail.com",
            "homepage": "https://github.com/MrHash",
            "role": "Developer"
        },
        {
            "name": "Thorsten Schmitt-Rink",
            "email": "schmittrink@gmail.com",
            "homepage": "https://github.com/shrink0r",
            "role": "Developer"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dailex/dlx-testing"
        }
    ],
    "require": {
        "php": ">=7.1",
        "daikon/async-job": "0.1",
        "daikon/config": "0.1",
        "daikon/couchdb-adapter": "0.1",
        "daikon/elasticsearch5-adapter": "0.1",
        "daikon/entity": "0.1",
        "daikon/event-sourcing": "0.1",
        "daikon/flysystem-adapter": "0.1",
        "daikon/rabbitmq3-adapter": "0.1",
        "daikon/read-model": "0.1",
        "dailex/dlx-security": "0.1",
        "dailex/dlx-testing": "0.1",
        "monolog/monolog": "^1.0",
        "psr/container": "^1.0",
        "rdlowrey/auryn": "^1.4.0",
        "roave/security-advisories": "master@dev",
        "silex/silex": "^2.0",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/asset": "^3.0",
        "symfony/config": "^3.0",
        "symfony/console": "^3.0",
        "symfony/filesystem": "^3.0",
        "symfony/form": "^3.0",
        "symfony/serializer": "^3.0",
        "symfony/translation": "^3.0",
        "symfony/twig-bridge": "^3.0",
        "symfony/validator": "^3.0",
        "symfony/yaml": "^3.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "silex/web-profiler": "^2.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Dailex\\": "app/lib/"
        }
    },
    "bin": [
        "bin/dailex",
        "bin/dailex.php"
    ],
    "scripts": {
        "code-sniffer": "phpcs --extensions=php --standard=psr2 app",
        "code-sniffer-autofix": "phpcbf --standard=psr2 -d tabWidth=4 app"
    }
}